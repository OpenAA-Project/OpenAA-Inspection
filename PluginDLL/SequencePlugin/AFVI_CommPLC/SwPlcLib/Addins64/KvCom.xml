<?xml version="1.0" encoding="utf-8"?>
<addin>
  <presets>
    <preset type="SERIAL" name="KVSeries">
      <Option>StationNo=0;ReadMaxSize=10;WriteMaxSize=10</Option>
    </preset>
    <preset type="TCP/IP,UDP/IP" name="KVSeries">
      <Option>StationNo=0;ReadMaxSize=1000;WriteMaxSize=1000</Option>
    </preset>
  </presets>
  <optionItems>
    <optionItem name="StationNo" type="SERIAL,TCP/IP,UDP/IP" range="(0 - 9) (-1:無し)" description="局番号"/>
    <optionItem name="ReadMaxSize" type="SERIAL,TCP/IP,UDP/IP" range="1-1000" description="読込み最大点数"/>
    <optionItem name="WriteMaxSize" type="SERIAL,TCP/IP,UDP/IP" range="1-1000" description="書込み最大点数"/>
  </optionItems>
</addin>
